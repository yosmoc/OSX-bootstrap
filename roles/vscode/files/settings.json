// Place your settings in this file to overwrite the default settings
{
    "workbench.welcome.enabled": false,
    "editor.copyWithSyntaxHighlighting": false,
    "files.autoSave": "onFocusChange",
    "editor.minimap.enabled": true,
    "git.autofetch": false,
    "workbench.iconTheme": "vs-seti",
    "workbench.editor.enablePreview": false,
    "editor.renderWhitespace": "all",
    "files.trimTrailingWhitespace": true,
    "editor.snippetSuggestions": "top",
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.quickSuggestionsDelay": 0,
    "window.commandCenter": true,
    "search.exclude": {
        "**/.git": true,
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.bundle.js*": true
    },
    "emmet.includeLanguages": {
        "javascript": "javascriptreact",
        "typescript": "typescriptreact"
    },
    "emmet.syntaxProfiles": {
        "jsx": {
            "self_closing_tag": true
        },
        "tsx": {
            "self_closing_tag": true
        }
    },
    "emmet.triggerExpansionOnTab": true,
    "[javascript]": {
        "editor.formatOnSave": true,
        "editor.formatOnPaste": true
    },
    "[javascriptreact]": {
        "editor.formatOnSave": true,
        "editor.formatOnPaste": true
    },
    "[typescript]": {
        "editor.formatOnSave": true,
        "editor.formatOnPaste": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode",
    },
    "[typescriptreact]": {
        "editor.formatOnSave": true,
        "editor.formatOnPaste": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode",
    },
    "[python]": {
        "editor.formatOnSave": false,
        "editor.formatOnPaste": false,
        "editor.formatOnType": false,
        "editor.defaultFormatter": "ms-python.python",
    },
    "go.useLanguageServer": true,
    "[go]": {
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true,
        },
    },
    "[go.mod]": {
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true,
        },
    },
    "gopls": {
        "usePlaceholders": true,
        "staticcheck": false,
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[terraform]": {
      "editor.defaultFormatter": "hashicorp.terraform",
      "editor.formatOnSave": true,
      "editor.formatOnSaveMode": "file"
    },
    "[terraform-vars]": {
      "editor.defaultFormatter": "hashicorp.terraform",
      "editor.formatOnSave": true,
      "editor.formatOnSaveMode": "file"
    },
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "cSpell.userWords": [
        "Kubernetes",
        "asdecimal",
        "contentful",
        "getenv",
        "ioexpanders",
        "jsonify",
        "lund",
        "mysqlconnector",
        "nums",
        "prodtest",
        "pynarutils",
        "silabs",
        "sqlite",
        "strftime",
        "strptime",
    ],
    "python.jediEnabled": false,
    "python.languageServer": "Pylance",
    "python.analysis.completeFunctionParens": true,
    "python.analysis.autoImportCompletions": true,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook",
        "file:/**/*.parquet": "jupyter-data-wrangler"
    },
    "security.workspace.trust.untrustedFiles": "open",
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "editor.fontSize": 12,
    "insertDateString.formatTime": "hh:mm",
    "files.insertFinalNewline": true,
    "dev.containers.defaultExtensions": [
        "eamodio.gitlens",
        "Rubymaniac.vscode-paste-and-indent",
        "ericmccarthy.vs-code-mark-point",
    ],
    "remote.SSH.defaultExtensions": [
        "eamodio.gitlens",
        "Rubymaniac.vscode-paste-and-indent",
        "ericmccarthy.vs-code-mark-point",
    ],
    "terminal.integrated.allowChords": false,
    "go.delveConfig": {
        "dlvLoadConfig": {
            "followPointers": true,
            "maxVariableRecurse": 1,
            "maxStringLen": 64,
            "maxArrayValues": 64,
            "maxStructFields": -1
        },
        "apiVersion": 2,
        "showGlobalVariables": false,
        "debugAdapter": "legacy",
        "substitutePath": []
    },
    "go.formatTool": "goimports",
    "editor.inlineSuggest.enabled": true,
    "go.toolsManagement.autoUpdate": true,
    "jupyter.askForKernelRestart": false,
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs":"active",
    "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?_",
    "editor.guides.indentation": true,
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "typescript.updateImportsOnFileMove.enabled": "always",
    "github.copilot.enable": {
        "*": true,
        "plaintext": true,
        "markdown": false,
        "scminput": false,
        "yaml": false
    },
    "editor.stickyScroll.enabled": true,
    "terminal.integrated.scrollback": 1000000,
    "python.analysis.typeCheckingMode": "basic",
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "terminal.integrated.shellIntegration.enabled": false,
    "terminal.integrated.commandsToSkipShell": [
      "-workbench.action.quickOpen",
      "-workbench.action.showAllSymbols"
    ],
    "githubPullRequests.pullBranch": "never",
    "editor.selectionClipboard": true,
    "typeChallenges.workspaceFolder": "/Users/yoshihisa.mochihara/.typeChallenges",
    "editor.largeFileOptimizations": false,
    "playwright.reuseBrowser": false,
    "screencastMode.onlyKeyboardShortcuts": true,
    "workbench.colorTheme": "Visual Studio Dark",
    "editor.emptySelectionClipboard": false,
    "terminal.integrated.copyOnSelection": true,
    "git.suggestSmartCommit": false,
    "playwright.showTrace": false,
    "copyOnSelect.copyOnKeyboardSelection": true,
    "copyOnSelect.copyOnMouseSelection": true,
    "copyOnSelect.trimStart": true,
    "copyOnSelect.trimEnd": true,
    "terminal.integrated.macOptionClickForcesSelection": true,
    "[go][go.mod]": {
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit"
        }
    },
    "gitlens.advanced.messages": {
        "suppressBlameInvalidIgnoreRevsFileWarning": true
    },
    "codeQL.telemetry.enableTelemetry": false,
    "workbench.tree.stickyScrollMaxItemCount": 10,
}
